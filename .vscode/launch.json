{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Vue: npm run",
            "command": "npm run serve",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceRoot}/frontend/src/",
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "api/src/app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "console": "integratedTerminal",
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
            ],
            "jinja": true
        },
        {
            "name": "Python: ML tracker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/api/src/ml_tracker.py",
            "console": "integratedTerminal",
            "args": [],
            "justMyCode": false
        },
        {
            "name": "Python: Analysis",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/api/src/analysis.py",
            "console": "integratedTerminal",
            "args": [],
            "justMyCode": true
        }
    ]
}